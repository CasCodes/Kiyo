
{
    "name": "Kiyo",
    "version": "0.1",
    "description": "alpha",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["script/content.js"]
        }
    ],
    "permissions": [
        "tabs"
    ],
    
    "background": {
        "scripts": ["script/background.js"]
    },

    "browser_action": {
        "default_popup": "widget.html",
        "default_icon": "res/icon.png"
    }
}
