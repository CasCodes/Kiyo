
{
    "name": "Kiyo",
    "version": "0.1",
    "description": "alpha",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },

    "browser_action": {
        "default_popup": "widget.html",
        "default_icon": "icon.png"
    }
}
